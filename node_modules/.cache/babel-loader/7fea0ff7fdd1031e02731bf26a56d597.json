,EAAA;AADA;AAJA;AA7CA,CAAA","sourcesContent":["<template>\n    <div class=\"change-file\">\n        <van-cell-group v-if=\"!is_upload\">\n            <van-field v-model=\"datas\" :placeholder=\"placeHolder\" clearable/>\n        </van-cell-group>\n\n        <van-uploader v-model=\"fileList\" multiple v-if=\"is_upload\"/>\n        <div class=\"save-btn\" @click=\"saveData\">保存</div>\n        <!--{{detail}} {{curPop}} {{placeHolder}}-->\n    </div>\n</template>\n\n<script>\n  export default {\n    name: \"InputItem\",\n    data() {\n      return {\n        name: '',\n        datas: this.detail,\n        is_upload: false,\n        fileList: [\n          { url: 'https://img.yzcdn.cn/vant/leaf.jpg' },\n          // Uploader 根据文件后缀来判断是否为图片文件\n        ]\n      }\n    },\n    methods: {\n      saveData() {\n        let data ={\n          datas: this.datas,\n          status: false,\n          curPop:this.curPop\n        }\n        this.$emit('changeStatus', data)\n      }\n    },\n    computed: {\n      placeHolder() {\n        if (this.curPop === 'name') {\n          return this.pHolder = '请输入店铺名'\n        }\n        if (this.curPop === 'addrDetail') {\n          return this.pHolder = '请输入详细地址'\n        }\n        if (this.curPop === 'user') {\n          return this.pHolder = '请输入联系人'\n        }\n        if (this.curPop === 'phone') {\n          return this.pHolder = '请输入联系人号码'\n        }\n        if (this.curPop === 'desc') {\n          return this.pHolder = '请输入店铺简介'\n        }\n        if(this.curPop === 'photo') {\n          return this.is_upload = true\n        }\n      }\n    },\n    props: {\n      detail: {\n        type: String\n      },\n      curPop: {\n        type: String\n      }\n    }\n  }\n</script>\n\n<style lang=\"stylus\" scoped>\n    .change-file {\n        position fixed;\n        top: 0;\n        left: 0;\n        right: 0;\n        bottom: 0;\n        background #f4f4f4\n    }\n\n    .save-btn {\n        width: 60%;\n        margin: 0 auto;\n        border-radius: 40px;\n        text-align: center;\n        font-size: 14px;\n        color: #fff;\n        padding: 10px;\n        background: #ea3756;\n        box-shadow: 0 0 3px 0 #ea3756;\n        position: fixed;\n        left:0;\n        right:0;\n        bottom: 40px;\n    }\n\n    .change-file .van-uploader {\n        padding: 30px 16px;\n    }\n</style>"],"sourceRoot":"src/views/shop/component"}]}